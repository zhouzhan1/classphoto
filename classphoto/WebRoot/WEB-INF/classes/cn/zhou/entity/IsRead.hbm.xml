<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- 1 配置类和表对应 
		class标签
		name属性：实体类全路径
		table属性：数据库表名称
	-->
	<class name="cn.zhou.entity.IsRead" table="zz_isread">
		<id name="rid" column="rid">
			<generator class="native"></generator>
		</id>
		<property name="informId" column="informId"></property>
		<property name="uid" column="uid"></property>
		<property name="state" column="state"></property>
	
	<many-to-one name="inform" class = "cn.zhou.entity.Inform" column="ir_id"></many-to-one>
	</class>
</hibernate-mapping>





